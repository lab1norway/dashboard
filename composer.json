{
    "name": "buildup/dashboard",
    "description": "A dashboard for your application using TailwindCSS",
    "license": "license",
    "authors": [{
        "name": "Jaime Castrillo",
        "email": "jc@lab1.no",
        "homepage": "https://www.lab1.no"
    }],
    "homepage": "https://github.com/buildup/dashboard",
    "keywords": ["Laravel", "Dashboard"],
    "require": {
        "illuminate/support": "~5|~6"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "BuildUp\\Dashboard\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BuildUp\\Dashboard\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BuildUp\\Dashboard\\DashboardServiceProvider"
            ],
            "aliases": {
                "Dashboard": "BuildUp\\Dashboard\\Facades\\Dashboard"
            }
        }
    }
}
